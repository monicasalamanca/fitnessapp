{
  "name": "fitnessapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .tsx || true",
    "prettier-format": "prettier --config .prettierrc src/**/*.tsx --write || true"
  },
  "eslintConfig": {
    "extends": "react-app"
